#include <stdio.h>
int main() {
    int num, temp,sum = 0;
    printf("Enter:");
    scanf("%d", &num);
    temp = num;
    while (temp > 0) {
        sum  = sum+(temp%10);
        temp = temp/10;}
    if (num % sum == 0)
        printf("%dNaive\n");
    else
        printf("%dnot naive\n");
    return 0;
}
  
