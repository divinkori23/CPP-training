#include<stdio.h>
#include<math.h>
#include<stdlib.h>
int main(){
    float x,y,pwr;
    printf("enter X:");
    scanf("%f",&x);
    printf("enter Y:");
    scanf("%f",&y);    
    printf("for the given mathematical expression the answer is:");
	pwr = pow(x,y);
    float numerator = (sin(x)+cos(x)+tan(x));
    float denominator = (log10(pwr))
    float ans = (numerator/ denominator);
    printf("%.2f",ans);
    return 0;   
}
