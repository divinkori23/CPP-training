#include <stdio.h>
#include<math.h>
int main() {
    float a,b,c,x1,x2;
    printf("Enter a,b,c:");
    scanf("%f %f %f",&a,&b,&c);
    x1=(((-b)+(sqrt(pow(b,2))-(4*a*c)))/(2*a));
    x2=(((-b)-(sqrt(pow(b,2))-(4*a*c)))/(2*a));
    printf("answer of x1:%f",x1);
    printf("answer of x2:%f",x2);
    return 0;
}
