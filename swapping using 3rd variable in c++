#include <iostream>
using namespace std;

void swap(float &num1, float &num2) {
    float temp = num1;
    num1 = num2;
    num2 = temp;
}

int main() {
    float num1, num2;
    cout << "Enter the first number: ";
    cin >> num1;
    cout << "Enter the second number: ";
    cin >> num2;

    cout << "\nBefore swapping:" << endl;
    cout << "First number (num1): " << num1 << endl;
    cout << "Second number (num2): " << num2 << endl;

    swap(num1, num2); 

    cout << "\nAfter swapping:" << endl;
    cout << "First number (num1): " << num1 << endl;
    cout << "Second number (num2): " << num2 << endl;

    return 0;
}
