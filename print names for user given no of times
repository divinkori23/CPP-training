#include <stdio.h> 
int main() {
    char name[50];
    int num_times;
    printf("Enter your name: ");
        scanf("%s", name);

        printf("Enter the number of times to print your name: ");
        scanf("%d", &num_times);

     for (int i = 0; i < num_times; i++) {
        printf("%s\n", name);
    }

    return 0; 
}
