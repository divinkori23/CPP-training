#include<stdio.h>
#include<math.h>
#include<stdlib.h>
int main(){
    float x,y;
    printf("enter X:");
    scanf("%f",&x);
    printf("enter Y:");
    scanf("%f",&y);    
    printf("for the given mathematical expression the answer is:");
    float numerator = (log(x)+exp(y));
    float denominator = (log10(x));
    float ans = (numerator/ denominator);
    float ans1=sqrt(ans);
    printf("%.2f",ans1);
    return 0;
}
