#include <iostream>
using namespace std;

int febterm(int n)
{
    int a=1,b=1,c,count=2;
    if(n==1 || n == 2)
        return 1;
    while(1)
    {
        count++;
        c=a+b;
        a=b;
        b=c;
        if(count == n)
            return c;
    }
        
}
int main() {
    int n,x;
    cout<<"enter the number:"<<endl;
    cin>>n;
    x=febterm(n);
    cout<<x;
    return 0;
}
