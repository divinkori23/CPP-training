// Online C++ compiler to run C++ program online
#include <iostream>
#include <math.h>
using namespace std;
float ans(float &x,float &y)
{
    float numera = log10(exp(x))+log(sqrt(y));
    float denom = sin(x)+exp(y)+pow(x,y);
    float answer= numera/denom;
    return answer;
}
int main() {
    float x,y,t;
    cout<<"enter x and y:"<<endl;
    cin>>x>>y;
    t=ans(x,y);
    cout<<t;
    return 0;
}
