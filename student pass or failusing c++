// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
bool grade(int a,int b,int c)
{
    if(a>35 && b>35 && c>35)
    {
        cout<<"you are the passed!"<<endl;
        return true;
    }
    else
    {
        cout<<"you are failed!!!"<<endl;
        return false;
    }
    return 0;
}
int main() {
    int a,b,c,t;
    cout<<"enter marks of 3 subjects:"<<endl;
    cin>>a>>b>>c;
    t=grade(a,b,c);
     cout<<t;
    return 0;
}
