// Online C++ compiler to run C++ program online
#include <iostream>
#include<math.h>
using namespace std;
float root(float &a,float &b,float &c)
{
    float d = sqrt((b*b)-(4*a*c));
    float x1=(-b+d)/(2*a);
    float x2=(-b-d)/(2*a);
    cout<<"roots are:"<<x1<<x2;
    return 0;
}
int main() {
    float a,b,c,x1,x2,t;
    cout<<"enter a,b and c:"<<endl;
    cin>>a>>b>>c;
    t=root(a,b,c);
    return 0;
}
